<%- include('./header.ejs') %>


<h1>Edit Student Registration</h1>

<% if(typeof message !== 'undefined'){%>
    <p style="color: red;">
        <%= message %>
    </p>
        <%}%>
        
        
        <div class="row px-5">

            <form action="/admin/edit-user-reg" method="post" style="display: contents;" autocomplete="off">
              
                <div id="formInq " class=" container-fluid col-md-6 ">
                    <div class="form-floating mb-2 ">
                        <input type="text" class="form-control" name="studentName" id="floatingName" placeholder="" value="<%=users.studentName %>" autocomplete="flase">
                        <label for="floatingName">Name of Student</label>
                      </div>

                      <div class="form-floating mb-2">
                        <input type="date" class="form-control" name="dob" id="floatingDOB" placeholder="" value="<%=users.dob %>" autocomplete="off">
                        <label for="floatingDOB">Date of Birth</label>
                      </div>

                      <div class="form-floating mb-2 ">
                        <input type="text" class="form-control" name="fatherName" id="floatingName" placeholder=" " value="<%=users.fatherName %>"  autocomplete="off">
                        <label for="floatingName">Father's Name</label>
                      </div>
                      <div class="form-floating mb-2 ">
                        <input type="text" class="form-control" name="motherName" value="<%=users.motherName %>" id="floatingName" placeholder=" " autocomplete="off" >
                        <label for="floatingName">Mother's Name</label>
                      </div>

                      <div class="form-floating mb-2 ">
                        <input type="text" value="<%=users.occupation %>" class="form-control" name="occupation" id="floatingName" placeholder=" " autocomplete="off" >
                        <label for="floatingName">Occupation</label>
                      </div>

                      <div class="form-floating mb-2 mb-5" >
                        <input type="text" class="form-control" name="address" placeholder="" value="<%=users.address %>" id="floatingTextarea2" style="height: 80px" value=""  autocomplete="off"></input>
                        <label for="floatingTextarea2">Address</label>
                      </div>
                      <div class="row g-3">
                        <div class="form-floating mb-2 col" >
                          <h6>Gender:</h6>
                        </div>
                          <div class="form-check col">
                              <input class="form-check-input" name="gender" value="male" type="radio" name="flexRadioDefault" id="RadioMale" <% if(users.gender == 'Male'){%> checked <% }%> >
                              <label class="form-check-label" for="RadioMale">
                                Male
                              </label>
                            </div>
                            <div class="form-check col">
                              <input class="form-check-input" name="gender" value="female" type="radio" name="flexRadioDefault" id="RadioFemale" <% if(users.gender == 'Female'){%> checked <% }%>>
                              <label class="form-check-label" for="RadioFemale">
                                Female
                              </label>
                            </div>
                        </div>
                      
                      
                      <div class="form-floating mb-2 ">
                        <input type="email" value="<%=users.email %>" name="email" class="form-control" id="floatingEmail" placeholder="" autocomplete="off">
                        <label for="floatingEmail">Email address</label>
                      </div>
                      
                    </div>
                      <div class="container-fluid col-md-6 d-inline" id="formInq">
                        <div class="row g-3  mb-2">
                          <div class="col-md">
                          <div class="form-floating mb-2 ">
                            <select id="mpDomicile" class="form-select mb-2" name="mpDomicile" required>
                              <option value="">Select</option>
                              <% const optionsOfDomicile = ["Yes","No"] %>
                            <% optionsOfDomicile.forEach((elem)=>{ %>
                    <option <% if(elem == users.mpDomicile){%>selected <%}%> value="<%= elem%>"><%= elem%></option>       
                              <% }) %>
                  
  
                            </select>
                            <label for="floatingEmail" class="d-flex">MP Domicile <h5 style="color: red;">*</h5></label>
                          </div>
                          </div>
                          <div class="col-md">
                            <div class="form-floating mb-2 ">
                              <select type="text" name="category" class="form-select" id="floatingInput" placeholder="Category" required>
                                <option value="" >Select </option>
                                <% const optionsOfCategory = ['SC','ST','OBC','EWS','General'] %>
                            <% optionsOfCategory.forEach((elem)=>{ %>
                    <option <% if(elem == users.category){%>selected <%}%> value="<%= elem%>"><%= elem%></option>       
                              <% }) %>
                              </select>
                              <label for="floatingSelect" class="d-flex">Category<h5 style="color: red;">*</h5></label>
                            </div>
                          </div>
                          </div>
  

                        
                      

                        <div class="form-floating mb-2">
                            <input type="number" class="form-control" name="mobile" id="floatingContact" placeholder="" value="<%=users.mobile%>" autocomplete="off">
                            <label for="floatingContact">Contact No.</label>
                          </div>
                          
                          <div class="form-floating mb-2 ">
                            <input type="number" name="guardianMobile" class="form-control" id="floatingGC" placeholder="" value="<%=users.guardianMobile%>" autocomplete="off">
                            <label for="floatingGC">Guardian Contact</label>
                          </div>

                           <h5>JEE Mains</h5>
                    <div class="form-floating mb-2">
                      <input type="text" class="form-control" id="floatingInputGrid" name="jeeEnrollmentNumber" placeholder="" value="<%=users.jeeMains.jeeEnrollmentNumber%>" autocomplete="off">
                      <label for="floatingInputGrid">Enrollment Number</label>
                    </div>
                    <div class="form-floating mb-2">
                      <input type="text" class="form-control" id="floatingInputGrid" name="jeeMarksPercentile" placeholder="" value="<%=users.jeeMains.jeeMarksPercentile%>" autocomplete="off">
                      <label for="floatingInputGrid">Marks/Percentile </label>
                    </div>
                    <div class="form-floating mb-2">
                      <input type="text" class="form-control" id="floatingInputGrid" name="jeeRank" placeholder="" value="<%=users.jeeMains.jeeRank%>" autocomplete="off">
                      <label for="floatingInputGrid">Rank </label>
                    </div>

                        
  
                           <h6>Course & Branch (in order of preference)</h6>
              
              
  
                      

                        <div class="row g-2  mb-2">
                          
                          <div class="col-md ">
                            <div class="form-floating ">
                              <label for="state-select"><h6>Select State:</h6></label>
                            </div>
                          </div>
                          <div class="col-md">      
                        <select id="state-select" class="form-select" name="state">
                          <option value="">Select</option>
                        </select>
                          </div>
                        </div>

                        <div class="row g-2 ">
                          
                          <div class="col-md">
                            <div class="form-floating ">
                              <label for="city-select "><h6>Select City:</h6></label>
                            </div>
                          </div>
                          <div class="col-md">      
                            <select id="city-select" class="form-select mb-2" name="city">
                              <option value="">Select</option>
                            </select>
                          </div>
                        </div>               
                      

                      </div>
                      
                      <div class="container-fluid col-md-6" id="formInq">
            <h5>Educational Qualification</h5>
            <h5>10th:</h5>

            
            <div class="row g-2 mb-2">
              
              <div class="col-md">
                  <input type="hidden" class="form-control" id="floatingInputGrid" placeholder="" name="degreeName" value="10th" autocomplete="off">

                <div class="form-floating">
                  <input type="text" class="form-control" id="floatingInputGrid" placeholder="" value="<%=users.qualifyingDegree.percentage[0]%>" name="percentage" autocomplete="off">
                  <label for="floatingInputGrid">(%)/CGPA</label>
                </div>

              </div>
              <div class="col-md">
                <div class="form-floating">
                  <input type="text" class="form-control" id="floatingInputGrid" placeholder="" name="examinationAuthority" value="<%=users.qualifyingDegree.examinationAuthority[0]%>" autocomplete="off">
                  <label for="floatingInputGrid">Examination Authority</label>
                </div>

              </div>
            </div>

            <div class="row g-2 mb-2">
              
              <div class="col-md">
              <div class="form-floating mb-2">
                <input type="text" class="form-control" name="passingYear" value="<%=users.qualifyingDegree.passingYear[0]%>" id="floatingInputGrid" placeholder="" autocomplete="off">
                <label for="floatingInputGrid">Year of Passing*</label>
              </div>
              </div>
              
            </div>


            <h5>12th:</h5>

            
            <div class="row g-2 mb-2">
                  <input type="hidden" class="form-control" id="floatingInputGrid" placeholder="" name="degreeName" value="12th" autocomplete="off">
              
              <div class="col-md">
                
                <div class="form-floating">
                  <input type="text" class="form-control" id="floatingInputGrid" placeholder="" name="percentage" value="<%=users.qualifyingDegree.percentage[1]%>" autocomplete="off">
                  <label for="floatingInputGrid">(%)/CGPA</label>
                </div>

              </div>
              <div class="col-md">
                <div class="form-floating">
                  <input type="text" class="form-control" value="<%=users.qualifyingDegree.examinationAuthority[1]%>" id="floatingInputGrid" placeholder="" name="examinationAuthority" autocomplete="off">
                  <label for="floatingInputGrid">Examination Authority</label>
                </div>

              </div>
            </div>

            <div class="row g-2 mb-2">

              <div class="col-md">
                <div class="form-floating mb-2">
                  <input type="text" value="<%=users.qualifyingDegree.subjectStream[0]%>" class="form-control" name="subjectStream" id="floatingInputGrid" placeholder="stream" autocomplete="off">
                  <label for="floatingInputGrid">Stream</label>
                </div>
                </div>
              
              <div class="col-md">
              <div class="form-floating mb-2">
                <input type="text" class="form-control" name="passingYear" value="<%=users.qualifyingDegree.passingYear[1]%>" id="floatingInputGrid" placeholder="" autocomplete="off">
                <label for="floatingInputGrid">Year of Passing*</label>
              </div>
              </div>
              
            </div>

            <h5>Graduation:</h5>

            
            <div class="row g-2 mb-2">
                  <input type="hidden" class="form-control" id="floatingInputGrid" placeholder="" name="degreeName" value="Graduation" autocomplete="off">
              
              <div class="col-md">
                <div class="form-floating">
                  <input type="text" class="form-control" id="floatingInputGrid" placeholder="" name="percentage" value="<%=users.qualifyingDegree.percentage[2]%>" autocomplete="off">
                  <label for="floatingInputGrid">(%)/CGPA</label>
                </div>

              </div>
              <div class="col-md">
                <div class="form-floating">
                  <input type="text" class="form-control" id="floatingInputGrid" placeholder="" value="<%=users.qualifyingDegree.examinationAuthority[2]%>" name="examinationAuthority" autocomplete="off">
                  <label for="floatingInputGrid">College Name</label>
                </div>

              </div>
            </div>

            <div class="row g-2 mb-2">
              
              <div class="col-md">
              <div class="form-floating mb-2">
                <input type="text" class="form-control" name="passingYear" id="floatingInputGrid" placeholder="" value="<%=users.qualifyingDegree.passingYear[2]%>" autocomplete="off">
                <label for="floatingInputGrid">Year of Passing*</label>
              </div>
              </div>
              
            </div>

          </div>
                    <div class="container-fluid col-md-6">


                      <div class="form-floating mb-2">
                <select class="form-select" id="select-course" name="course" aria-label="Floating label select example">
                  <option selected>Select</option>
                  
                </select>
                <label for="floatingSelect">Course</label>
              </div>
              <div class="row g-3  mb-2">
                
                <div class="col-md">      
                  <div class="form-floating mb-2">
                    
                    <select class="form-select select-branch" id="select-branch1" name="branch" aria-label="Floating label select example" style="display: none;">
                      <option selected>Select</option>
                      
                    </select>
                    <label for="floatingSelect" id="branchLabel1" style="display: none ;">Branch</label>
                  </div>
                </div>
                <div class="col-md">      
                  <div class="form-floating mb-2">
                    <select class="form-select select-branch" id="select-branch2" name="branch" aria-label="Floating label select example" style="display: none;">
                      <option selected>Select</option>
                      
                    </select>
                    <label for="floatingSelect" id="branchLabel2" style="display: none ;">Branch</label>
                  </div>
                </div>

                <div class="col-md">      
                  <div class="form-floating ">
                    <select class="form-select select-branch" id="select-branch3" name="branch" aria-label="Floating label select example" style="display: none;">
                      <option selected>Select</option>
                      
                    </select>
                    <label for="floatingSelect" id="branchLabel3" style="display: none ;">Branch</label>
                  </div>
                </div>
               
               
              </div>
                      <h5>Registration Fee Details</h5>

                      <div class="form-floating mb-2">
                        <select class="form-select" name="paymentMode" id="floatingSelectGrid" aria-label="Floating label select example">
                          <option selected>Select</option>
                          <% const optionOfPayment = ['Cash','Cheque','Online'] %>
                          <% optionOfPayment.forEach((elem)=>{ %>
                    <option <% if(elem == users.registrationFee.paymentMode){%> selected <%}%> value="<%= elem%>"><%= elem%></option>
                                
                          <% }) %>
                        </select>
                        <label for="floatingSelectGrid">Payment Mode</label>
                      </div>

                      <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="floatingInputGrid" name="amount" placeholder="" value="<%=users.registrationFee.amount%>" autocomplete="off">
                        <label for="floatingInputGrid">Amount </label>
                      </div>

                      <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="floatingInputGrid" name="receiptNumber" placeholder="" value="<%=users.registrationFee.receiptNumber%>" autocomplete="off">
                        <label for="floatingInputGrid">Receipt Number </label>
                      </div>

                      <div class="form-floating mb-2">
                        <input type="date" class="form-control" id="floatingInputGrid" name="receiptDate" placeholder="" value="<%=users.registrationFee.receiptDate%>" autocomplete="off">
                        <label for="floatingInputGrid">Receipt Date </label>
                      </div>


                      <h6>Opted Scheme Benefits:</h6>
                      <div class="btn-group mb-2" role="group" aria-label="Basic checkbox toggle button group">
                        <% const optionsOfBenefits = ['Early Bird','Merit List','Special Case','TFW','EWS']%>
                        <% optionsOfBenefits.forEach((elem,idx)=>{ %>
                          <input type="checkbox" class="btn-check" name="schemeOpted" value="<%=elem%>" id="btncheck<%=idx%>" autocomplete="off">
                          <label class="btn btn-outline-danger"  for="btncheck<%=idx%>"><%=elem%></label>

                       <% })%>
                      
                       
                      </div>

                      <h6>Additional Details:
                      </h6>

                      <div class="btn-group mb-2" role="group" aria-label="Basic checkbox toggle button group">
                        <% const optionsOfAdditional = ['Hostel','Bus Exeption']%>
                        <% let count = 6; %>
                        <% optionsOfAdditional.forEach((elem)=>{ %>
                          <input type="checkbox" class="btn-check" name="schemeOpted" value="<%=elem%>" id="btncheck<%=count%>" autocomplete="off">
                          <label class="btn btn-outline-danger"  for="btncheck<%=count++%>"><%=elem%></label>

                       <% })%>
          
                      </div>
                      <div class="row g-3  mb-2">
                        <div class="col-md">
                        <div class="form-floating mb-2 ">
                          <input type="text" name="referralName" class="form-control" id="floatingEmail" placeholder="" autocomplete="off">
                          <label for="floatingEmail">Referral Name</label>
                        </div>
                        </div>
                        <div class="col-md">
                        <div class="form-floating mb-2 ">
                          <input type="text" name="facultyName" class="form-control" id="floatingEmail" placeholder="" autocomplete="off">
                          <label for="floatingEmail">Faculty Name</label>
                        </div>
                        </div>
                        </div>
                      <br>
                      <button class="btn btn-primary" id="submitBtn" type="submit">Submit</button>
                    </div>





                      </form>
           </div>


        </div>


 <script src="../../Scripts/script.js"></script>
    <script src="../../Scripts/stateCity.js"></script>
    <script src="../../Scripts/course.js"></script>

